{
  "name": "zerda",
  "version": "0.1.0-dev",
  "private": true,
  "description": "Discord bot that assigns roles to people who are playing a game, so they sort higher in the users list",
  "main": "src/main.js",
  "scripts": {
    "dev": "ZERDA_DATABASE=dev.sqlite3 node src/main.js",
    "docker:daemon:start": "npm run docker:start -- --detach",
    "docker:daemon:stop": "docker-compose down",
    "docker:start": "docker-compose up --build",
    "knex": "npx knex --knexfile src/database/knexfile.js",
    "register": "node bin/register.js",
    "start": "NODE_ENV=prod node src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mimickal <mimickal.dev@gmail.com>",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/Mimickal/Zerda",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mimickal/Zerda.git"
  },
  "bugs": {
    "url": "https://github.com/Mimickal/Zerda/issues"
  },
  "dependencies": {
    "discord-command-registry": "^1.1.0",
    "discord.js": "^13.1.0",
    "knex": "^0.95.11",
    "minimist": "^1.2.5",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  }
}
